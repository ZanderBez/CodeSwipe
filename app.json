{
  "expo": {
    "name": "CodeSwipe",
    "slug": "codeswipe",
    "owner": "zanderdev",
    "scheme": "codeswipe",
    "version": "1.0.0",
    "orientation": "landscape",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "bundleIdentifier": "com.zander.codeswipe",
      "supportsTablet": true
    },
    "android": {
      "package": "com.zander.codeswipe",
      "adaptiveIcon": {
        "foregroundImage": "./assets/icon.png",
        "backgroundColor": "#000000"
      },
      "edgeToEdgeEnabled": true
    },
    "web": {
      "favicon": "./assets/icon.png"
    },
    "extra": {
      "eas": {
        "projectId": "6045ceaa-a839-4312-9857-609daf3c6eea"
      }
    },
    "plugins": [
      "expo-web-browser"
    ]
  }
}
